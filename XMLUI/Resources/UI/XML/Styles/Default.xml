<?xml version="1.0" encoding="utf-8"?>
<Style>
  
  <!--Fonts-->
  <Font Name="NanumBarunGothicBold" Asset="UI/Fonts/NanumBarunGothicBold" />

  <!--Image-->
  <Image Style="Default" Name="Image" Image="" Type="Simple" Width="100" Height="100" />
	
  <!--Panels-->
  <Panel Style="Default" Image="UI/Images/frm_tabOut" Type="Sliced" Width="800" Height="600" />
  
  <!--Buttons-->
  <Button Style="Default" Image="UI/Images/btn_blue" Down="UI/Images/btn_gray" Type="Sliced" Width="240" Height="62" />

  <Button Style="Blue" Image="UI/Images/btn_blue" Down="UI/Images/btn_gray" Type="Sliced" Width="240" Height="62" />
  
  <Button Style="Red" Image="UI/Images/btn_red" Down="UI/Images/btn_gray" Type="Sliced" Width="240" Height="62" />
  
  <Button Style="Close" Image="UI/Images/btn_close" Type="Simple" Width="45" Height="45" />

  <!--Text-->
  <Text Style="Default" 
        Font="NanumBarunGothicBold" 
        Size="24" 
        Spacing="0.7" 
        Color="0xfff5da" 
        Down="0xcccccc"
        Alignment="MiddleCenter" 
        AutoWrap="true" 
        BestFit="true" 
        MinSize="10" 
        MaxSize="24"
        Text="text" />
  
  <Text Style="ButtonLabel"
        Font="NanumBarunGothicBold"
        Size="24"
        Spacing="0.7"
        Color="0xfff5da"
        Down="0xcccccc"
        Alignment="MiddleCenter"
        AutoWrap="true"
        BestFit="true"
        MinSize="10"
        MaxSize="24"
        Text="Button"
        Anchor="Stretch" 
        Margin="30,30,0,2" />

	<Toggle Style="Default" Name="Toggle" Width="240" Height="62" />

	<Image Style="ToggleBG" Name="ToggleBG" Image="UI/Images/ico_checkBox" Type="Sliced" Width="53" Height="53" Anchor="MiddleLeft" />
	<Image Style="ToggleOn" Name="ToggleOn" Image="UI/Images/img_toggle_on" Type="Simple" Width="31" Height="31" />
	
</Style>